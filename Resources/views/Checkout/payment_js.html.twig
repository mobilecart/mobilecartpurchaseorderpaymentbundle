<script type="text/javascript">

    checkout.paymentHandlers['{{ code }}'] = function(buttonEl) {
        var code = "{{ code }}";

        var numberEl = $('#payment-' + code).find('input[name="' + code + '[reference_nbr]"]');
        var poNumber = numberEl.val();

        return {
            payment_method: code,
            "po[reference_nbr]": poNumber
        };
    }

</script>

